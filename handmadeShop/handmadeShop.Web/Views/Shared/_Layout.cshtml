<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Site Metas -->
    <title>ThewayShop - Ecommerce Bootstrap 4 HTML Template</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Site Icons -->
    <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="images/apple-touch-icon.png">

    <!-- Bootstrap CSS -->
    @Styles.Render("~/bundles/css/bootstrap.min.css")
    <!-- Site CSS -->
    @Styles.Render("~/bundles/css/style.css")
    <!-- Responsive CSS -->
    @Styles.Render("~/bundles/css/responsive.css")
    <!-- Custom CSS -->
    @Styles.Render("~/bundles/css/custom.css")

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>
    <div class="main-top">
        @Html.Partial("_MainTop")
    </div>

    <header class="main-header">
        @Html.Partial("_Header")
    </header>

    <div class="top-search">
        @Html.Partial("_Search")
    </div>

    <div id="slides-shop" class="cover-slides">
        @Html.Partial("_Slider")
    </div>

    @RenderBody()

    <footer>
        @Html.Partial("_Footer")
    </footer>

    <div class="footer-copyright">
        @Html.Partial("_FooterCopyright")
    </div>

    <a href="#" id="back-to-top" title="Back to top" style="display: none;">&uarr;</a>

    <!-- ALL JS FILES -->
    @Scripts.Render("~/bundles/js/jquery-3.2.1.min.js")
    @Scripts.Render("~/bundles/js/popper.min.js")
    @Scripts.Render("~/bundles/js/bootstrap.min.js")
    <!-- ALL PLUGINS -->
    @Scripts.Render("~/bundles/js/jquery.superslides.min.js")
    @Scripts.Render("~/bundles/js/bootstrap-select.js")
    @Scripts.Render("~/bundles/js/inewsticker.js")
    @Scripts.Render("~/bundles/js/bootsnav.js")
    @Scripts.Render("~/bundles/js/images-loaded.min.js")
    @Scripts.Render("~/bundles/js/isotope.min.js")
    @Scripts.Render("~/bundles/js/owl.carousel.min.js")
    @Scripts.Render("~/bundles/js/baguetteBox.min.js")
    @Scripts.Render("~/bundles/js/form-validator.min.js")
    @Scripts.Render("~/bundles/js/contact-form-script.js")
    @Scripts.Render("~/bundles/js/custom.js")
</body>
</html>
