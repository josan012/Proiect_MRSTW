                <div class="col-md-7 col-sm-12 text-center ftco-animate">
                    <h1 class="mb-3 mt-5 bread">Blog</h1>
                    <p class="breadcrumbs"><span class="mr-2">@Html.ActionLink("Home","Index","Home")</span> <span>Blog</span></p>
                </div>

            </div>
        </div>
    </div>
</section>

